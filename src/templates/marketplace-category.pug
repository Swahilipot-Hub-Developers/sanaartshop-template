          input(type='text' placeholder='Search in this category...').form-control.border-0.shadow-none

        // Sort
        .d-flex.align-items-center
          .dropdown.py-4.border-start
            a(href='#' data-bs-toggle='dropdown').nav-link-style.fs-md.fw-medium.dropdown-toggle.p-4
              span.d-inline-block.py-1
                i.ci-thumb-up.align-middle.opacity-60.mt-n1.me-2
                | Popular
            ul.dropdown-menu.dropdown-menu-end
              li
                a(href='#').dropdown-item
                  i.ci-flag.me-2.opacity-60
                  | Newest
              li
                a(href='#').dropdown-item
                  i.ci-rocket.me-2.opacity-60
                  | Bestsellers
              li
                a(href='#').dropdown-item
                  i.ci-loading.me-2.opacity-60
                  | Updated
              li
                a(href='#').dropdown-item
                  i.ci-arrow-down.me-2.opacity-60
                  | Low Price
              li
                a(href='#').dropdown-item
                  i.ci-arrow-up.me-2.opacity-60
                  | High Price

