extends partials/layout

//- Override default config variables
append config
  - var pageTitle = 'Marketplace category'
  - var breadcrumbs = [{'title': 'Market', 'link': 'home-marketplace.html'}, {'title': 'Inside category'}]
  - var parentMenu = 'Shop'

//- Navbar
