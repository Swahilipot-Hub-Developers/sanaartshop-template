          span.fs-md.text-nowrap.me-4 Pages&nbsp; 1 / 5
          a(href='#').nav-link-style.p-4.border-start
            span.d-inline-block.py-1
              i.ci-arrow-left
          a(href='#').nav-link-style.p-4.border-start
            span.d-inline-block.py-1
              i.ci-arrow-right

