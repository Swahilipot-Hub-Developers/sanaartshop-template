extends partials/layout

//- Override default config variables
append config
  - var vendorStyles = ['lightgallery/css/lightgallery-bundle.min.css']
  - var vendorScripts = ['lightgallery/lightgallery.min.js', 'lightgallery/plugins/fullscreen/lg-fullscreen.min.js', 'lightgallery/plugins/zoom/lg-zoom.min.js']
  - var pageTitle = 'Single Product / Brand Store'
  - var parentMenu = 'Home'

//- Navbar
block navbar

  // Floating Navbar (Transparent version)
  // Remove "navbar-sticky" class to make navigation bar scrollable with the page.
  header.navbar.navbar-sticky.navbar-expand-lg.navbar-light.bg-light
    .container
      a(href='index.html').navbar-brand.d-none.d-sm-block.me-4.order-lg-1
        img(src='img/logo-dark.png' width='142' alt='Cartzilla')
      a(href='index.html').navbar-brand.d-sm-none.me-2.order-lg-1
        img(src='img/logo-icon.png' width='74' alt='Cartzilla')
      
      .navbar-toolbar.d-flex.align-items-center.order-lg-3
        button(type='button' data-bs-toggle='collapse' data-bs-target='#navbarCollapse').navbar-toggler
          span.navbar-toggler-icon
        a(href='#signin-modal' data-bs-toggle='modal').navbar-tool.ms-1.me-n1
          span.navbar-tool-tooltip Account
          .navbar-tool-icon-box: i.navbar-tool-icon.ci-user
        .navbar-tool.dropdown.ms-3
          a(href='#').navbar-tool-icon-box.bg-secondary.dropdown-toggle
            i.navbar-tool-icon.ci-cart
          .dropdown-menu.dropdown-menu-end.text-center.py-4.px-3
            div(style='width: 16rem;')
              img(src='data:image/png;base64,' width='40' alt='Empty cart').d-inline-block.mb-2
              p.fs-sm.text-muted.mb-0 Your cart is currently empty

      
      #navbarCollapse.collapse.navbar-collapse.me-auto.order-lg-2

        // Models dropdown
        ul.navbar-nav.navbar-mega-nav.ms-lg-4.pe-lg-2.me-lg-2.pt-3.pt-lg-0
          li.nav-item.dropdown
            a(href='#' data-bs-toggle='dropdown').nav-link.dropdown-toggle.ps-lg-0
              i.ci-view-grid.me-2
              | Models
            .dropdown-menu.px-2.ps-0.pb-4
              .d-flex.flex-wrap.flex-sm-nowrap
                .mega-dropdown-column.pt-4.px-3
                  .widget.mb-2
                    a(href='#').d-block.mb-3
                      img(src='img/home/mono-product/models/01.jpg' alt='Model image')
                    h6.fs-base.text-center.mb-2 Solo Headphones
                .mega-dropdown-column.pt-4.px-3
                  .widget.mb-2
                    a(href='#').d-block.mb-3
                      img(src='img/home/mono-product/models/02.jpg' alt='Model image')
                    h6.fs-base.text-center.mb-2 Air Earphones
                .mega-dropdown-column.pt-4.px-3
                  .widget.mb-2
                    a(href='#').d-block.mb-3
                      img(src='img/home/mono-product/models/03.jpg' alt='Model image')
                    h6.fs-base.text-center.mb-2 Headphones Pro

        //- menu-primary.pug
        include partials/menu-primary

  
