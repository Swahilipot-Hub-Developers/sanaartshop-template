extends partials/layout

//- Override default config variables
append config
  - var vendorStyles = ['nouislider/dist/nouislider.min.css']
  - var vendorScripts = ['nouislider/dist/nouislider.min.js']
  - var pageTitle = 'Catalog v.1'
  - var breadcrumbs = [{'title': 'NFT Marketplace', 'link': 'home-nft.html'}, {'title': 'All NFTs'}]
  - var parentMenu = 'Catalog'

//- Navbar
block navbar
  include partials/navbar-nft


//- Page Content
block content

  // Page Title
  .bg-accent.pt-4.pb-5
    .container.pt-2.pb-3.pt-lg-3.pb-lg-4
      .d-lg-flex.justify-content-between.pb-3
        .order-lg-2.mb-3.mb-lg-0.pt-lg-2
          +breadcrumb('light')
        .order-lg-1.pe-lg-4.text-center.text-lg-start
          h1.h3.text-light.mb-0 !{pageTitle}

  .container.pb-5.mb-2.mb-md-4

    // Toolbar
    .bg-light.shadow-lg.rounded-3.p-4.mt-n5.mb-4
      .row.gy-3.gx-4.justify-content-between
        .col-lg-2.col-md-3.col-sm-5.col-12.order-md-1.order-sm-2.order-3
          .dropdown
            a(href='#shop-filters' data-bs-toggle='collapse').btn.btn-outline-secondary.dropdown-toggle.w-100
              i.ci-filter.me-2
              | Filters
        .col-md.col-12.order-md-2.order-sm-1.order-1
          .input-group
            input(type='text' placeholder='Search collection, title or user...').form-control.pe-5.rounded
            i.ci-search.position-absolute.top-50.end-0.translate-middle-y.zindex-5.me-3

        .col-lg-3.col-md-4.col-sm-7.col-12.order-md-3.order-sm-3.order-2
          .d-flex.align-items-center.flex-shrink-0
            label.form-label.mb-0.me-2.pe-1.fw-normal.text-nowrap.d-sm-block.d-none
              | Sort by:
            select.form-select
              option(selected disabled) Newest
              option Oldest

      //- nft-filters-collapse.pug template
      include partials/nft-filters-collapse


    // Products grid
    .row.row-cols-lg-4.row-cols-md-3.row-cols-sm-2.row-cols-1.gy-sm-4.gy-3.pt-sm-3

      // Product
      .col.mb-2
        article.card.h-100.border-0.shadow
          .card-img-top.position-relative.overflow-hidden
            a(href='nft-single-auction-live.html').d-block
              img(src='img/nft/catalog/01.jpg' alt='Product image')

            // Countdown timer
            .badge.bg-dark.m-3.fs-sm.position-absolute.top-0.start-0.zindex-5
              i.ci-time.me-1
              .countdown.d-inline(data-countdown='12/31/2022 12:00:00 PM')
                span.countdown-hours.mb-0.me-0
                  span.countdown-value 0
                  span.countdown-label.fs-lg :
                span.countdown-minutes.mb-0.me-0
                  span.countdown-value 0
                  span.countdown-label.fs-lg :
                span.countdown-seconds.mb-0.me-0
                  span.countdown-value 0

            // Wishlist button
            button(type='button' data-bs-toggle='tooltip' data-bs-placement='left' title='Add to Favorites' style='margin: 12px;').btn-wishlist.btn-sm.position-absolute.top-0.end-0
              i.ci-heart

          .card-body
            h3.product-title.mb-2.fs-base
              a(href='nft-single-auction-live.html').d-block.text-truncate
                | 3d aesthetics with shapes
            span.fs-sm.text-muted Current bid:
            .d-flex.align-items-center.flex-wrap
              h4.mt-1.mb-0.fs-base.text-darker 0.156 ETH
              span.mt-1.ms-1.fs-xs.text-muted (≈ $ 595.76)

          .card-footer.mt-n1.py-0.border-0
            .d-flex.align-items-center.position-relative.mb-1.py-3.border-top
              img(src='img/nft/catalog/avatars/01.png' width='32' alt='Avatar').me-2.rounded-circle
              a(href='nft-vendor.html').nav-link-style.fs-sm.stretched-link @foxnet_creator

